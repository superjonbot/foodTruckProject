{
  "name": "@globalstuff/eslint-config-node",
  "version": "4.0.0",
  "description": "eslint configs for Node",
  "main": "./dist/index.js",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist/index.js"
  ],
  "scripts": {
    "build": "tsup index.js",
    "publish-package": "npm publish",
    "version-package": "yarn version",
    "test": "node --experimental-vm-modules $(yarn bin jest) .",
    "test:update": "node --experimental-vm-modules $(yarn bin jest) -u ."
  },
  "dependencies": {
    "@globalstuff/prettier-config": "3.0.0",
    "@eslint/compat": "1.0.3",
    "@eslint/js": "9.1.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsonc": "2.15.1",
    "eslint-plugin-markdownlint": "0.6.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "globals": "15.1.0",
    "prettier": "3.0.0"
  },
  "devDependencies": {
    "shared-configs": "*"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "peerDependencies": {
    "eslint": "8.57.0 || 9.x"
  },
  "prettier": "@globalstuff/prettier-config"
}
